<?xml version="1.0"?>
<launch>

  <env name="ROSCONSOLE_FORMAT" value="[${severity}] [${time}] [${node}]: ${message}"/>

  <!-- sensors that are not used yet -->
  <node pkg="my_ariac" 
        type="my_ariac_sensors_node" 
        name="my_sensors_node"
        output="screen"></node>

  <!-- 4 stations -->
  <node pkg="my_ariac" 
        type="my_ariac_station_node" 
        name="my_as1_node"
        args="as1"
        output="screen"></node>

  <node pkg="my_ariac" 
        type="my_ariac_station_node" 
        name="my_as2_node"
        args="as2"
        output="screen"></node>

  <node pkg="my_ariac" 
        type="my_ariac_station_node" 
        name="my_as3_node"
        args="as3"
        output="screen"></node>

  <node pkg="my_ariac" 
        type="my_ariac_station_node" 
        name="my_as4_node"
        args="as4"
        output="screen"></node>

  <!-- 4 agvs -->
  <node pkg="my_ariac" 
        type="my_ariac_agv_node" 
        name="my_agv1_node"
        args="agv1"
        output="screen"></node>

  <node pkg="my_ariac" 
        type="my_ariac_agv_node" 
        name="my_agv2_node"
        args="agv2"
        output="screen"></node>

  <node pkg="my_ariac" 
        type="my_ariac_agv_node" 
        name="my_agv3_node"
        args="agv3"
        output="screen"></node>

  <node pkg="my_ariac" 
        type="my_ariac_agv_node" 
        name="my_agv4_node"
        args="agv4"
        output="screen"></node>

  <!--  factory manager -->
  <node pkg="my_ariac" 
        type="my_ariac_node" 
        name="group1_rwa1" output="screen"></node>

</launch>
